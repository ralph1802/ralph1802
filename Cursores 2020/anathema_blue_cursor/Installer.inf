text		= "Text Select (Anathema Blue).ani"
hand		= "Handwriting (Anathema Blue).ani"
unavailiable	= "Unavailable (Anathema Blue).cur"
vert		= "Vertical Resize (Anathema Blue).ani"
horz		= "Horizontal Resize (Anathema Blue).ani"
dgn1		= "Diagonal Resize 1 (Anathema Blue).ani"
dgn2		= "Diagonal Resize 2 (Anathema Blue).ani"
